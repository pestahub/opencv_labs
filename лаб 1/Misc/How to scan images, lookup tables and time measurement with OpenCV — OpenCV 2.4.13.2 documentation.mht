p">[</span><span class=3D"o">*</span><span class=3D"n">it</span><span cl=
ass=3D"p">];</span>
            <span class=3D"k">break</span><span class=3D"p">;</span>
        <span class=3D"p">}</span>
    <span class=3D"k">case</span> <span class=3D"mi">3</span>:
        <span class=3D"p">{</span>
            <span class=3D"n">MatIterator_</span><span class=3D"o">&lt;<=
/span><span class=3D"n">Vec3b</span><span class=3D"o">&gt;</span> <span =
class=3D"n">it</span><span class=3D"p">,</span> <span class=3D"n">end</s=
pan><span class=3D"p">;</span>
            <span class=3D"k">for</span><span class=3D"p">(</span> <span=
 class=3D"n">it</span> <span class=3D"o">=3D</span> <span class=3D"n">I<=
/span><span class=3D"p">.</span><span class=3D"n">begin</span><span clas=
s=3D"o">&lt;</span><span class=3D"n">Vec3b</span><span class=3D"o">&gt;<=
/span><span class=3D"p">(),</span> <span class=3D"n">end</span> <span cl=
ass=3D"o">=3D</span> <span class=3D"n">I</span><span class=3D"p">.</span=
><span class=3D"n">end</span><span class=3D"o">&lt;</span><span class=3D=
"n">Vec3b</span><span class=3D"o">&gt;</span><span class=3D"p">();</span=
> <span class=3D"n">it</span> <span class=3D"o">!=3D</span> <span class=3D=
"n">end</span><span class=3D"p">;</span> <span class=3D"o">++</span><spa=
n class=3D"n">it</span><span class=3D"p">)</span>
