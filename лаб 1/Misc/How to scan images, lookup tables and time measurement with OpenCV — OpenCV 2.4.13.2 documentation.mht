    <span class=3D"n">uchar</span> <span class=3D"n">table</span><span c=
lass=3D"p">[</span><span class=3D"mi">256</span><span class=3D"p">];</sp=
an>
    <span class=3D"k">for</span> <span class=3D"p">(</span><span class=3D=
"kt">int</span> <span class=3D"n">i</span> <span class=3D"o">=3D</span> =
<span class=3D"mi">0</span><span class=3D"p">;</span> <span class=3D"n">=
i</span> <span class=3D"o">&lt;</span> <span class=3D"mi">256</span><spa=
