    /*background-color: #ecf0f3;*/
    padding: 0 1px 0 1px;
    font-size: 1.4em;
}

div.math p {
    margin-top: 10px;
    margin-bottom: 10px;
}

dl.function > dt:first-child {
    margin-bottom: 7px;
}

dl.cfunction > dt:first-child {
